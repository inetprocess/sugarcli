{
    "name": "inetprocess/sugarcli",
    "description": "SugarCli is a command line tool to install and manage SugarCRM installations.",
    "license": "Apache-2.0",
    "keywords": ["sugarcrm", "sugarcli"],
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanuel.dyan@inetprocess.com"
        },
        {
            "name": "RÃ©mi Sauvat",
            "email": "remi.sauvat@inetprocess.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/rsauvatinet/mysqldump-php"
        },
	{
	    "type": "vcs",
            "url": "https://github.com/inetprocess/libsugarcrm"
	},
	{
	    "type": "vcs",
            "url": "https://github.com/inetprocess/libinventoryclient"
	}
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-Reflection": "*",
        "csanquer/colibri-csv" : "^1.1",
        "guzzlehttp/guzzle": "~6.5.8",
        "inetprocess/libinventoryclient": "v2.1.3",
        "inetprocess/libsugarcrm": "v2.0.2",
        "edyan/neuralyzer": "^4.1",
        "jms/serializer": "~0.16",
        "padraic/phar-updater": "^1.0",
        "symfony/config": "^5",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5",
        "symfony/filesystem": "^5",
        "symfony/finder": "^5",
        "symfony/process": "^5",
        "symfony/stopwatch": "^5",
        "symfony/yaml": "^5",
        "webmozart/path-util": "^2.3",
        "padraic/humbug_get_contents": "1.0.4",
        "ifsnop/mysqldump-php": "dev-master",
        "symfony/property-access": "^2.8",
        "psr/log": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "SugarCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SugarCli\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.6"
        }
    },
    "bin": ["bin/sugarcli"]
}
